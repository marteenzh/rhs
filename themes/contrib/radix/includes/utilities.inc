<?php
/**
 * @file
 * Helper functions for the Radix theme.
 */

/**
 * Returns Bootstrap panel classes.
 */
function _radix_get_bootstrap_panel_classes() {
  return array(
    'panel-default',
    'panel-primary',
    'panel-success',
    'panel-info',
    'panel-warning',
    'panel-danger'
  );
}
